@model Casgem_CodeFirstProject.DAL.Entities.Admin
@{
    ViewBag.Title = "UpdateGuide";
    Layout = "~/Views/Shared/TemplateView.cshtml";
}

<div class="alert alert-primary" role="alert">
    Giriş yapan kullanıcı: <u> @Html.Raw("@")@Session["userTravel"].ToString()</u>
    <form action="/Login/Exit" method="post">
        <button type="submit" class="btn btn-danger">Çıkış Yap</button>
    </form>
</div>
<h2>Admin Güncelleme Sayfası</h2>


<form method="post">
    @Html.HiddenFor(x=> x.AdminID)
    <label>Rehber Ad Soyad</label>
    @Html.TextAreaFor(x => x.UserName, new { @class = "form-control" })
    <br />
    <label>Rehber Ünvanı</label>
    @Html.TextAreaFor(x => x.Password, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Güncelle</button>
</form>